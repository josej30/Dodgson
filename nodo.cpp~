#include <iostream>
#include <vector>

typedef std::vector<int> PerfVector;

using namespace std;

class Nodo {
  PerfVector perfil;

public:
  void insertar(int, int);
  int comprobar(int x, int y) {return x== y;}
  int valor(int);
};

void Nodo::insertar(int val,int pos) {
  int myarray [] = {val};
  perfil.insert (perfil.end(), myarray, myarray+1);
}

int Nodo::valor(int x){
  return perfil[x];

}

int main () {
  Nodo nodo;

  nodo.insertar(3,1);
  // for (int i = 0; i< nodo.size(); i++){
  //  cout << nodo[i];
  // }


  cout << "rect area: " << nodo.valor(1) << endl;
  return 0;
}
